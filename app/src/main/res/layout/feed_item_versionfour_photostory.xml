<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/listlayout_taball"
    android:paddingBottom="5dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/whitegradient"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/feed_item_margin"
            android:orientation="vertical"
            >
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">



                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/feed_item_margin"
                    android:orientation="vertical"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.android.volley.toolbox.NetworkImageView
                            android:id="@+id/feedImage_photostory"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:maxHeight="275dp"
                            android:minHeight="275dp"
                            android:scaleType="centerCrop"
                            android:adjustViewBounds="true"
                            android:visibility="visible" />

                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <com.android.volley.toolbox.NetworkImageView
                                android:id="@+id/feedImage_photostory_three_one"
                                android:layout_width="match_parent"
                                android:drawingCacheQuality="low"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:layout_height="150dp"
                                android:adjustViewBounds="true"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />
                            <com.android.volley.toolbox.NetworkImageView
                                android:id="@+id/feedImage_photostory_three_two"
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_height="150dp"
                                android:layout_margin="5dp"
                                android:adjustViewBounds="true"
                                android:drawingCacheQuality="low"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />

                        </LinearLayout>
                        <com.android.volley.toolbox.NetworkImageView
                            android:id="@+id/feedImage_photostory_three_three"
                            android:layout_width="match_parent"
                            android:layout_height="150dp"
                            android:layout_margin="5dp"
                            android:adjustViewBounds="true"
                            android:drawingCacheQuality="low"
                            android:scaleType="centerCrop"
                            android:visibility="visible" />


                    </LinearLayout>

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.android.volley.toolbox.NetworkImageView
                            android:id="@+id/feedImage_photostory_type_two_one"
                            android:layout_width="match_parent"
                            android:drawingCacheQuality="low"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:layout_height="300dp"
                            android:adjustViewBounds="true"
                            android:scaleType="centerCrop"
                            android:visibility="visible" />
                        <com.android.volley.toolbox.NetworkImageView
                            android:id="@+id/feedImage_photostory_type_two_two"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:layout_margin="5dp"
                            android:adjustViewBounds="true"
                            android:layout_height="300dp"
                            android:drawingCacheQuality="low"
                            android:scaleType="centerCrop"
                            android:visibility="visible" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <com.android.volley.toolbox.NetworkImageView
                                android:id="@+id/feedImage_photostory_type_four_one"
                                android:layout_width="match_parent"
                                android:drawingCacheQuality="low"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:layout_height="150dp"
                                android:adjustViewBounds="true"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />
                            <com.android.volley.toolbox.NetworkImageView
                                android:id="@+id/feedImage_photostory_type_four_two"
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_margin="5dp"
                                android:layout_height="150dp"
                                android:adjustViewBounds="true"
                                android:drawingCacheQuality="low"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />
                        </LinearLayout>
                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <com.android.volley.toolbox.NetworkImageView
                                android:id="@+id/feedImage_photostory_type_four_three"
                                android:layout_width="match_parent"
                                android:drawingCacheQuality="low"
                                android:layout_margin="5dp"
                                android:layout_weight="1"
                                android:layout_height="150dp"
                                android:adjustViewBounds="true"
                                android:scaleType="centerCrop"
                                android:visibility="visible" />
                            <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:layout_margin="5dp"
                                android:layout_height="wrap_content">
                                <com.android.volley.toolbox.NetworkImageView
                                    android:id="@+id/feedImage_photostory_type_four_four"
                                    android:layout_width="match_parent"
                                    android:adjustViewBounds="true"

                                    android:layout_height="150dp"
                                    android:drawingCacheQuality="low"
                                    android:scaleType="centerCrop"
                                    android:visibility="gone" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:id="@+id/more_images"
                                    android:textSize="15sp"
                                    android:layout_gravity="center_horizontal|center_vertical|center"
                                    android:textAlignment="center"
                                    android:textColor="@android:color/white"
                                    android:layout_height="wrap_content" />
                            </FrameLayout>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:id="@+id/qtypetitle_taball"

                        android:textColor="@color/red"

                        android:layout_width="wrap_content"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textStyle="bold" />
                    <ImageButton
                        android:visibility="gone"
                        android:id="@+id/arrow"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="10dp"
                        android:background="@android:color/transparent"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:id="@+id/qtypetitle_sourcename"

                        android:textColor="@color/red"

                        android:layout_width="wrap_content"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:layout_height="wrap_content"
                        android:textSize="12sp"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <TextView
                    android:layout_marginTop="4dp"
                    android:id="@+id/item_title_taball"
                    android:textColor="@color/white"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="-10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp"
                    android:paddingBottom="5dp"
                    android:layout_height="wrap_content"
                    android:textSize="26sp"
                    />
                    </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_marginRight="10dp"
                        android:visibility="gone"
                        android:id="@+id/date_taball"
                        android:textColor="@color/white"
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="5dp"
                        android:layout_height="wrap_content"
                        android:textSize="8sp"
                        />
                    <TextView
                        android:layout_marginTop="7dp"
                        android:layout_marginRight="10dp"
                        android:maxLines="3"
                        android:ellipsize="end"
                        android:id="@+id/textview_shortdescription"
                        android:textColor="#79ffffff"
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:paddingBottom="5dp"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="9dp"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_marginLeft="5dp"
                        android:layout_weight=".30"
                        android:orientation="horizontal"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">
                        <ImageButton
                            android:id="@+id/button_likes"
                            android:layout_width="0dp"
                            android:background="@android:color/transparent"
                            android:src="@mipmap/heart"
                            android:layout_weight=".10"
                            android:layout_height="32dp" />
                        <ImageButton
                            android:id="@+id/button_comment"
                            android:layout_width="0dp"
                            android:background="@android:color/transparent"
                            android:src="@mipmap/commentnew"
                            android:layout_weight=".10"
                            android:layout_height="32dp" />
                        <ImageButton
                            android:id="@+id/button_share"
                            android:layout_width="0dp"
                            android:background="@android:color/transparent"
                            android:src="@mipmap/sharenew"
                            android:layout_weight=".10"
                            android:layout_height="32dp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight=".20"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                    </LinearLayout>
                    <LinearLayout
                        android:layout_gravity="right"
                        android:layout_weight=".41"

                        android:layout_marginTop="10dp"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <TextView

                            android:layout_marginTop="5dp"
                            android:id="@+id/alltab_commentscount"
                            android:textColor="@android:color/white"
                            android:layout_marginRight="10dp"
                            android:gravity="right|center"
                            android:textSize="12dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />
                        <TextView

                            android:id="@+id/alltab_likescount"
                            android:layout_marginTop="5dp"
                            android:textColor="@android:color/white"
                            android:gravity="right|center"
                            android:layout_marginRight="5dp"
                            android:layout_alignParentRight="true"
                            android:textSize="12dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content" />

                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_marginTop="7dp"
                    android:id="@+id/counts_layout"
                    android:orientation="vertical"
                    android:layout_height="wrap_content">
                </RelativeLayout>
                <RelativeLayout

                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:minHeight="2dp"
                    android:layout_height="wrap_content">
                    <View
                        android:id="@+id/line_separter"
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:background="@android:color/transparent"/>
                </RelativeLayout>
                <TableLayout
                    android:layout_marginTop="3dp"
                        android:visibility="gone"
                    android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content"    android:focusable="true" android:focusableInTouchMode="true" >
                    <TableRow   android:layout_width="match_parent"
                        android:layout_height="wrap_content" >
                        <Button android:layout_width="0dp"  android:background="@color/whitefood" android:layout_margin="2dp"  android:drawableLeft="@mipmap/like" android:paddingRight="23dp"  android:layout_height="30dp"  android:id="@+id/taball_likes"
                            android:textColor="@color/white"  android:textSize="12dp"  android:layout_weight=".33" />

                        <Button android:layout_width="0dp"  android:background="@color/whitefood" android:layout_margin="2dp" android:paddingRight="10dp" android:drawableLeft="@mipmap/comment"  android:layout_height="30dp"  android:id="@+id/taball_comment"
                            android:textColor="@color/white"  android:textSize="12dp"  android:layout_weight=".34"  />

                        <Button android:layout_width="0dp" android:background="@color/whitefood" android:layout_margin="2dp" android:layout_height="30dp" android:paddingRight="17dp" android:drawableLeft="@mipmap/share"  android:id="@+id/taball_sharepost"
                            android:textColor="@color/white" android:textSize="12dp"  android:layout_weight=".33"/>
                        <Button android:layout_width="0dp" android:visibility="gone"  android:background="@color/whitefood" android:layout_margin="2dp" android:layout_height="30dp"   android:id="@+id/taball_savepage"
                            android:textColor="@color/white" android:textSize="12dp"  android:layout_weight="0" android:gravity="center" android:textAlignment="center"/>

                    </TableRow>
                </TableLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>










